int main(int argc, string[] argv) {
	hashtable[int,int] tbl = new hashtable_seq[int,int];
	int n = int_of_string(argv[1]);
	int y = int_of_string(argv[2]);
	int z = int_of_string(argv[3]);

	commute _ {
		{ 	busy_wait(n);
			if(ht_size(tbl) > 0) { 
			y = y + z;
		  	}
		}
		{ 	y = ht_get(tbl,z);
			busy_wait(n);
		}
	}
	
	return 0;
}
